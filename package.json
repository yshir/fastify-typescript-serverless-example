{
  "name": "fastify-typescript-serverless-template",
  "version": "1.0.0",
  "description": "fastify-typescript-serverless-template",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/bootstrap/server.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "eslint": "eslint . --ext .js,.ts ",
    "eslint:fix": "eslint . --ext .js,.ts --fix",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run eslint:fix",
      "npm run prettier:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yshir/fastify-typescript-serverless-template.git"
  },
  "keywords": [],
  "author": "yshir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yshir/fastify-typescript-serverless-template/issues"
  },
  "homepage": "https://github.com/yshir/fastify-typescript-serverless-template#readme",
  "dependencies": {
    "aws-lambda-fastify": "^1.4.4",
    "dotenv": "^8.2.0",
    "fastify": "^3.15.0",
    "fastify-cors": "^5.2.0",
    "fastify-favicon": "^3.1.0",
    "fastify-sensible": "^3.1.1",
    "fastify-swagger": "^4.7.0",
    "inversify": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "shallow-equal-object": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/aws-lambda-fastify": "^1.4.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.6.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsc-alias": "^1.2.10",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
